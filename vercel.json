{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "agent.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*\\.(?:css|js))", "dest": "/$1" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
